{
   "name": "owngitextension",
   "displayName": "OwnGitExtension",
   "description": "Provides tools for creating and changing GitHub repositories.",
   "author": "Philipp B.",
   "publisher": "Philipp B.",
   "license": "MIT",
   "version": "0.0.1",
   "repository": {
      "type": "github",
      "url": "https://github.com/phil1436/owngitextension"
   },
   "engines": {
      "vscode": "^1.74.0"
   },
   "categories": [
      "Other"
   ],
   "icon": "/resources/logo.png",
   "activationEvents": [
      "onCommand:owngitextension.createGitRepo",
      "onCommand:owngitextension.openOptions",
      "onCommand:owngitextension.addReadmeOverview",
      "onCommand:owngitextension.addNewVersion"
   ],
   "main": "./extension.js",
   "contributes": {
      "commands": [
         {
            "command": "owngitextension.createGitRepo",
            "title": "Create GitHub Repository Files",
            "category": "Own Git Extension"
         },
         {
            "command": "owngitextension.openOptions",
            "title": "Open Options File",
            "category": "Own Git Extension"
         },
         {
            "command": "owngitextension.addReadmeOverview",
            "title": "Add Readme Overview",
            "category": "Own Git Extension"
         },
         {
            "command": "owngitextension.addNewVersion",
            "title": "Add New Version",
            "category": "Own Git Extension"
         }
      ]
   },
   "scripts": {
      "lint": "eslint .",
      "pretest": "npm run lint",
      "test": "node ./test/runTest.js"
   },
   "devDependencies": {
      "@types/vscode": "^1.74.0",
      "@types/glob": "^8.0.0",
      "@types/mocha": "^10.0.0",
      "@types/node": "16.x",
      "eslint": "^8.26.0",
      "glob": "^8.0.3",
      "mocha": "^10.1.0",
      "typescript": "^4.8.4",
      "@vscode/test-electron": "^2.2.0"
   }
}
